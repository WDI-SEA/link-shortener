<!-- ALL LINKS links.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head %>
</head>
<body>

    <header>
        <% include partials/header %>
    </header>

    <main>
    <h3>Most popular links:</h3>
    <% link.forEach(function(url) { %>
        <div class="row">
            <div class="col s6">
            Site - <%= url.url %>
            Short link - localhost:4000/<%= url.hash %>
            Clicked on <%= url.clicks %> times
        </div>
    <% }); %>    
    </main>

    <footer>
        <% include partials/footer %>
    </footer>
    
</body>
</html>