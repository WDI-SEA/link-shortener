<div class="container">
	<div class="page-header">
		<h1>k.ninja <small>All my simplified urls</small></h1>
		<a href="/" class="btn btn-primary">Home</a>
	</div>
	<p>(by popularity)</p>
	<% urls.forEach(function(url) { %>
		<div class="well">
			<p>
				<a href="http://localhost:3000/<%= url.id %>">
					<%= url.hash %>
				</a>
			</p>
			<p>
				Clicks: <%= url.clicks %>
			</p>
		</div>
	<% }); %>
</div>